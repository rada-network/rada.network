{
  "compilerOptions": {
    "module": "esnext",
    "target": "es6",
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "@lib/*": ["./lib/*"],
      "@utils/*": ["./utils/*"],
      "@config/*": ["./config/*"],
      "@components/*": ["./components/*"],
      "@data/*": ["./data/*"],
      "@public/*": ["./public/*"],
      "@styles/*": ["./styles/*"]
    }
  },
  "exclude": [
    "node_modules",
    "**/node_modules/*",
    ".next",
    "https_cert",
    "public"
  ]
}
